{
  "private": true,
  "name": "@watilde/dotfiles",
  "version": "1.0.0",
  "description": "my dotfiles",
  "main": "index.js",
  "scripts": {
    "submodule-init": "git submodule update --init --recursive",
    "submodule-update": "git submodule foreach git pull origin master",
    "docker-build": "docker build -t 'chest:latest' .",
    "docker-kill": "docker kill $(docker ps -q)",
    "docker-run": "docker run -it -v ~/Development:/root/Development -v ~/.ssh:/root/.ssh chest:latest",
    "docker-rmi": "docker rmi -f $(docker images -q)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/watilde/dotfiles.git"
  },
  "author": "Daijiro Wachi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/watilde/dotfiles/issues"
  },
  "homepage": "https://github.com/watilde/dotfiles#readme",
  "dependencies": {
    "@gitignorejs/gitignore-cli": "^0.1.0",
    "alert-cli": "0.0.2",
    "chest": "^1.2.3",
    "clinic": "^2.2.0",
    "core-validate-commit": "^3.7.0",
    "cowsay": "^1.3.1",
    "emoji-cli": "^1.0.0",
    "gtop": "^1.0.0",
    "http-server": "^0.11.1",
    "source-code-pro": "git+https://github.com/adobe-fonts/source-code-pro.git#release",
    "tldr": "^3.2.5",
    "trash-cli": "^1.4.0"
  }
}
